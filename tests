//Stress test!

for(var i = 0; i < 1000; i++){
      var entity;
      boughtEntity = 'orcPeon';
	    var health = 100;
      var x = ~~(Math.random() * (levelWidth * size - 1));
      var y = ~~(Math.random() * (levelHeight * size - 1));
      while(isBlocked(x, y)){
        x = ~~(Math.random() * (levelWidth * size - 1));
        y = ~~(Math.random() * (levelHeight * size - 1));
      }
      entity = new Entity({
          'x': x,
          'y': y
      }, health, boughtEntity, playerId, playerColor);
      entity.id = cantor(Date.now(), ~~(performance.now()*1000))
      socket.emit('addEntity', {entity: entity});
      entities.push(entity);
      boughtEntity = false;}
